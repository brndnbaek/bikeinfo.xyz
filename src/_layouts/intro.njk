{% include "heroLarge.njk" %}
{% include "popularTags.njk" %}
	

	<div id="teamKorvelo" class="rounded relative w-5/6 mx-auto mt-5 md:mt-10 z-50 neomorph bg-gray-900">
		<a href="/korvelo" class="underline text-pink-500 z-50">
			<h1 class="ml-4 font-bold font-sans text-gray-300 text-base md:text-2xl z-50">
				Team Korvelo - Infomation & Stats</h1>
		</a>
	</div>

	<div class="mt-5 w-5/6 mx-auto flex flex-row flex-wrap z-30 justify-evenly">
		<div class="flex flex-col">
			<p class="text-gray-200 text-lg font-bold text-center">Latest News</p>
			{% for item in collections.news %}
			<a href="{{item.url|url}}" class="text-gray-400">- {{item.data.title}}</a>
			{%endfor%}
		</div>
		<div class="flex-col z-50 mb-2">
			<p class="text-gray-200 text-lg font-bold text-center">Upcoming Events</p>
			<iframe src="https://calendar.google.com/calendar/embed?height=400&amp;wkst=1&amp;bgcolor=%23090909&amp;ctz=America%2FNew_York&amp;src=aHMycGg3NWptZGZhYW9hcTJjMXBnNG10YmdAZ3JvdXAuY2FsZW5kYXIuZ29vZ2xlLmNvbQ&amp;color=%23616161&amp;showTitle=0&amp;showNav=0&amp;showPrint=0&amp;showTabs=0&amp;showCalendars=0&amp;showTz=0&amp;mode=AGENDA" 
			style="border-width:0"  height="400" frameborder="0" scrolling="no"
			class="w-11/12"></iframe>
		</div>
		<div class="z-50 mt-2">
			<p class="text-gray-200 text-lg font-bold mb-2 text-center">Weekly Stats</p>
			<iframe allowtransparency="true" frameborder='0' height='160' scrolling='no'
				src='https://www.strava.com/clubs/150475/latest-rides/1903597767594808fd16f324691a9a206e2a4cba?show_rides=false'
				 class="w-11/12"></iframe>
		</div>
	</div>


	<div id="nPlusOne" class="relative rounded w-5/6 mx-auto mt-5 md:mt-20 z-20 neomorph bg-gray-900">
		<a href="/bikes" class="underline text-pink-500">
			<h1 class="ml-4 font-bold font-sans text-gray-300 text-base md:text-2xl">
				Rule #12 - <i>The correct number of bikes to own is N + 1.</i> </h1>
		</a>
	</div>


	<section class="flex flex-row w-11/12 mx-auto mt-5" >
		<div class="overflow-x-scroll flex flex-row rounded w-11/12 mx-auto z-20">
			{% for bike in collections.bikes %}
			<div class="p-4 flex-col flex flex-shrink-0">
				<div class="relative">
					<h3 class="absolute text-white pt-4 left-0 ml-4"
						style="backdrop-filter:blur(5px) brightness(30%);
						-webkit-backdrop-filter:blur(5px) brightness(30%); 
						height:300px;writing-mode: vertical-rl; text-orientation:sideways">
						{{bike.data.title}}</h3>
				</div>
				<a href="{{bike.url}}"><img src="{{bike.data.heroImage}}" class="object-cover object-center"
						alt="{{bike.data.title}}" srcset="" style="width:250px; height:300px;"></a>
			</div>
			{% endfor %}

		</div>
	</section>

	<div class="relative rounded w-5/6 mx-auto mt-5 md:mt-20 neomorph bg-gray-900">
		<a href="/collections/nPlusOne" class="underline text-pink-500">
			<h1 class="ml-4 font-bold font-sans text-gray-300 text-base md:text-2xl">Quality Over Quantity
				</i> </h1>
		</a>
	</div>
	<section class="flex flex-row w-11/12 mx-auto mt-5" style="height:300px;">
		<div class="overflow-x-scroll flex flex-row rounded w-11/12 mx-auto z-20">
			<div class="p-4 flex-col flex flex-shrink-0">
				<div class="relative">
					<h3 class="absolute text-white pt-4 left-0 ml-4"
						style="-webkit-backdrop-filter:blur(5px) brightness(30%);backdrop-filter:blur(5px) brightness(30%);height:300px;writing-mode: vertical-rl; text-orientation:sideways">
						Tarmac SL7</h3>
				</div>
				<img src="https://source.unsplash.com/250x300/?parts" class="neomorph object-cover" alt="" srcset="">
			</div>

		</div>
	</section>
</body>
<script src="/static/app.bundled.js"></script>


</html>